  <select id="GetModuleDataInfo" resultMap="ModuleDataInfoDTO">
    SELECT
    M.Module_ID AS "moduleId",
    M.Module_Name AS "moduleName",
    M.upTime AS "upTime",
    M.Is_Calculate AS "isCalculate"
    ,File_URL AS "FileURl"
    ,LOG.getAnswNum
    FROM Module M
    LEFT JOIN(SELECT
    Log_Calculate.ModuleID As "moduleIds",count(*) AS "getAnswNum"
